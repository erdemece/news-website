<!DOCTYPE html>
<html lang="en">
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
		<meta charset="utf-8" />
		<title>Dashboard - {% block title %}{% endblock %}</title>

		<meta name="description" content="overview &amp; stats" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

		<!-- bootstrap & fontawesome -->
		<link rel="stylesheet" href="/skin/default/admin/css/bootstrap.css" />
		<link rel="stylesheet" href="/skin/default/admin/css/font-awesome.css" />

		<!-- page specific plugin styles -->
		{% block stylesheet %}
		{% endblock %}
		<!-- text fonts -->
		<link rel="stylesheet" href="/skin/default/admin/css/ace-fonts.css" />

		<!-- ace styles -->
		<link rel="stylesheet" href="/skin/default/admin/css/ace.css" class="ace-main-stylesheet" id="main-ace-style" />

		<!--[if lte IE 9]>
			<link rel="stylesheet" href="/skin/default/admin/css/ace-part2.css" class="ace-main-stylesheet" />
		<![endif]-->

		<!--[if lte IE 9]>
		  <link rel="stylesheet" href="/skin/default/admin/css/ace-ie.css" />
		<![endif]-->

		<!-- inline styles related to this page -->

		<!-- ace settings handler -->
		<script src="/skin/default/admin/js/ace-extra.js"></script>

		<!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->

		<!--[if lte IE 8]>
		<script src="/skin/default/admin/js/html5shiv.js"></script>
		<script src="/skin/default/admin/js/respond.js"></script>
		<![endif]-->
	</head>

	<body class="no-skin">
		{% include 'admin/header.twig' %}
		<!-- /section:basics/navbar.layout -->
		<div class="main-container ace-save-state" id="main-container">
			{% block base_content %}{% endblock %}
			<a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
				<i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
			</a>
		</div>

		{% include 'admin/footer.twig' %}

		{% block javascript %}

		<!-- basic scripts -->

		<!--[if !IE]> -->
		<script src="/skin/default/admin/js/jquery.js"></script>

		<!-- <![endif]-->

		<!--[if IE]>
		<script src="/skin/default/admin/js/jquery1x.js"></script>
		<![endif]-->
		<script type="text/javascript">
		  if('ontouchstart' in document.documentElement) document.write("<script src='/skin/default/admin/js/jquery.mobile.custom.js'>"+"<"+"/script>");
		</script>
		<script src="/skin/default/admin/js/bootstrap.js"></script>

		<!-- page specific plugin scripts -->
		<script src="/skin/default/admin/js/bootstrap-tag.js"></script>
		<script src="/skin/default/admin/js/jquery.hotkeys.js"></script>
		<script src="/skin/default/admin/js/bootstrap-wysiwyg.js"></script>

		<!-- ace scripts -->
		<script src="/skin/default/admin/js/ace/elements.scroller.js"></script>
		<script src="/skin/default/admin/js/ace/elements.colorpicker.js"></script>
		<script src="/skin/default/admin/js/ace/elements.fileinput.js"></script>
		<script src="/skin/default/admin/js/ace/elements.typeahead.js"></script>
		<script src="/skin/default/admin/js/ace/elements.wysiwyg.js"></script>
		<script src="/skin/default/admin/js/ace/elements.spinner.js"></script>
		<script src="/skin/default/admin/js/ace/elements.treeview.js"></script>
		<script src="/skin/default/admin/js/ace/elements.wizard.js"></script>
		<script src="/skin/default/admin/js/ace/elements.aside.js"></script>
		<script src="/skin/default/admin/js/ace/ace.js"></script>
		<script src="/skin/default/admin/js/ace/ace.ajax-content.js"></script>
		<script src="/skin/default/admin/js/ace/ace.touch-drag.js"></script>
		<script src="/skin/default/admin/js/ace/ace.sidebar.js"></script>
		<script src="/skin/default/admin/js/ace/ace.sidebar-scroll-1.js"></script>
		<script src="/skin/default/admin/js/ace/ace.submenu-hover.js"></script>
		<script src="/skin/default/admin/js/ace/ace.widget-box.js"></script>
		<script src="/skin/default/admin/js/ace/ace.settings.js"></script>
		<script src="/skin/default/admin/js/ace/ace.settings-rtl.js"></script>
		<script src="/skin/default/admin/js/ace/ace.settings-skin.js"></script>
		<script src="/skin/default/admin/js/ace/ace.widget-on-reload.js"></script>
		<script src="/skin/default/admin/js/ace/ace.searchbox-autocomplete.js"></script>

		<!-- inline scripts related to this page -->

		<!-- the following scripts are used in demo only for onpage help and you don't need them -->
		<link rel="stylesheet" href="/skin/default/admin/css/ace.onpage-help.css" />
		<link rel="stylesheet" href="/skin/default/admin/js/themes/sunburst.css" />

		<script type="text/javascript"> ace.vars['base'] = '..'; </script>
		<script src="/skin/default/admin/js/ace/elements.onpage-help.js"></script>
		<script src="/skin/default/admin/js/ace/ace.onpage-help.js"></script>
		<script src="/skin/default/admin/js/rainbow.js"></script>
		<script src="/skin/default/admin/js/language/generic.js"></script>
		<script src="/skin/default/admin/js/language/html.js"></script>
		<script src="/skin/default/admin/js/language/css.js"></script>
		<script src="/skin/default/admin/js/language/javascript.js"></script>



		{% endblock %}
	</body>
</html>
